<div id="app">
  <header id="header">
    <nav class="navbar navbar-inverse navbar-static-top">
      <div class="container">
        <div class="navbar-header">
          <a class="navbar-brand" href="/">Sensors App</a>
        </div>
        <div class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
          </ul>
        </div>
      </div>
    </nav>
  </header>
  <section id="main"></section>
  <%= javascript_tag do %>
    $(function() { SensorApp.start({
      config: {
        bootstrap: true
      },
      models: <%== @sensors.to_json %>
    }); });
  <% end %>
</div>
